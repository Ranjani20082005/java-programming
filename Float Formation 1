import java.util.*;

public class DisplyNumber {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        float floatNumber = sc.nextFloat();
        
        System.out.printf("%.6f\n",floatNumber);
        System.out.printf("%.4f\n", floatNumber);
  
        System.out.printf("%.2f\n", floatNumber);
        System.out.printf("%.0f\n", floatNumber);
        
        
        
        
       sc.close(); 
        
    }
}
